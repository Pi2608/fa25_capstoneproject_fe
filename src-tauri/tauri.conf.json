{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "IMOS",
  "version": "0.1.0",
  "identifier": "com.fa25.capstoneproject",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "node scripts/create-production-html.js",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": []
  },
  "app": {
    "windows": [
      {
        "title": "IMOS",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
